{
  "name": "@tsed/platform-adapter-kit-starter",
  "version": "7.10.1",
  "description": "A platform adapter",
  "source": "./src/index.ts",
  "main": "./lib/cjs/index.js",
  "module": "./lib/esm/index.js",
  "typings": "./lib/types/index.d.ts",
  "exports": {
    "types": "./lib/types/index.d.ts",
    "import": "./lib/esm/index.js",
    "require": "./lib/cjs/index.js",
    "default": "./lib/esm/index.js"
  },
  "scripts": {
    "build": "yarn barrels && yarn run build:esm && yarn run build:cjs",
    "build:cjs": "tsc --build tsconfig.compile.json",
    "build:esm": "tsc --build tsconfig.compile.esm.json",
    "barrels": "yarn barrelsby --delete -d ./src -e \"\\.spec\\.ts\" -e \"__mock__\" -e \".benchmark.ts\"",
    "start": "ts-node -r tsconfig-paths/register test/app/index.ts",
    "test": "cross-env NODE_ENV=test yarn jest --max-workers=2 --testTimeout 10000",
    "test:lint": "eslint '**/*.{ts,js}'",
    "test:lint:fix": "yarn test:lint --fix",
    "prepare": "is-ci || husky install"
  },
  "keywords": [
    "Platform",
    "Adapter",
    "TypeScript",
    "typescript",
    "Decorator",
    "decorators",
    "decorator",
    "Controller",
    "Inject",
    "ioc",
    "di",
    "mvc",
    "ES2015",
    "ES6",
    "server",
    "rest",
    "api",
    "validation"
  ],
  "author": {
    "name": "Romain Lenzotti"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/tsedio/tsed/issues"
  },
  "homepage": "http://tsed.io/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tsedio/tsed.git"
  },
  "dependencies": {
    "tslib": "2.4.1"
  },
  "devDependencies": {
    "@commitlint/cli": "17.4.2",
    "@commitlint/config-conventional": "17.4.2",
    "@tsed/ajv": "7.14.1",
    "@tsed/common": "7.14.1",
    "@tsed/core": "7.14.1",
    "@tsed/di": "7.14.1",
    "@tsed/exceptions": "7.14.1",
    "@tsed/json-mapper": "7.14.1",
    "@tsed/logger": "6.2.2",
    "@tsed/platform-cache": "7.14.1",
    "@tsed/platform-params": "7.14.1",
    "@tsed/platform-router": "7.14.1",
    "@tsed/platform-test-sdk": "7.14.1",
    "@tsed/platform-views": "7.14.1",
    "@tsed/schema": "7.14.1",
    "@types/jest": "^29.2.5",
    "@types/multer": "^1.4.7",
    "@types/superagent": "^4.1.16",
    "@typescript-eslint/eslint-plugin": "^5.11.0",
    "@typescript-eslint/parser": "^5.11.0",
    "ajv": "8.12.0",
    "axios": "1.3.0",
    "barrelsby": "^2.5.1",
    "cross-env": "7.0.3",
    "eslint": "^8.12.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-mocha": "10.1.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-workspaces": "0.7.0",
    "husky": "8.0.1",
    "is-ci": "^3.0.1",
    "jest": "^29.2.0",
    "lint-staged": "^12.3.2",
    "multer": "^1.4.5-lts.1",
    "prettier": "2.8.8",
    "semantic-release": "19.0.3",
    "superagent": "^8.0.9",
    "supertest": "6.2.2",
    "ts-jest": "^29.0.3",
    "ts-node": "10.8.1",
    "typescript": "4.9.4"
