    "@tsed/di": ">=7",
    "@tsed/exceptions": ">=7",
    "@tsed/json-mapper": ">=7",
    "@tsed/logger": ">=6.2.2",
    "@tsed/openspec": "^7",
    "@tsed/platform-cache": ">=7",
    "@tsed/platform-views": ">=7",
    "@tsed/schema": ">=7",
    "@types/multer": "^1.4.7"
  },
  "peerDependenciesMeta": {
